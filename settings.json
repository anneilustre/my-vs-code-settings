{
    "editor.formatOnPaste": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "editor.fontFamily": "'Dank Mono', 'Fira Code Retina', 'Consolas', monospace",
    "editor.fontLigatures": true,
    "editor.tokenColorCustomizations": {
      "textMateRules": [
        {
          "scope": [
            "comment",
            "entity.name.type.class",
            "keyword",
            "constant",
            "storage.modifier",
            "storage.type.class.js"
          ],
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": [
            "invalid",
            "keyword.operator",
            "constant.numeric.css",
            "keyword.other.unit.px.css",
            "constant.numeric.decimal.js",
            "constant.numeric.json"
          ],
          "settings": {
            "fontStyle": ""
          }
        }
      ]
    },
    "editor.fontWeight": "normal",
    "[html]": {
      "editor.defaultFormatter": "vscode.html-language-features"
    },
    "editor.formatOnSave": true,
    "editor.formatOnType": false,
    "editor.tabSize": 2,
    "editor.cursorStyle": "line",
    "[json]": {
      "editor.defaultFormatter": "vscode.json-language-features"
    },
    "emmet.includeLanguages": {
      "html": "html",
      "javascript": "javascriptreact"
    },
    "workbench.colorCustomizations": {
      // "editorCursor.foreground": "#e3256b",
      "editorCursor.foreground": "#ffff00",
      "terminalCursor.foreground": "#FB48C4"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "json.schemas": [],
    "editor.cursorBlinking": "expand",
    "window.zoomLevel": 1,
    "workbench.preferredLightColorTheme": "Oceanic Next Italic - White",
    "workbench.preferredDarkColorTheme": "Oceanic Next Italic",
    "window.autoDetectColorScheme": false,
    "workbench.colorTheme": "Oceanic Next Italic",
    "prettier.semi": false,
    "prettier.singleQuote": true,
    "prettier.jsxSingleQuote": true,
    "prettier.requireConfig": true,
    "javascript.format.semicolons": "insert",
    "[javascript]": {
      "editor.defaultFormatter": "vscode.typescript-language-features"
    }
  }
  